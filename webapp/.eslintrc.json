{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "react"
    ],
    "globals": {
        "$": false,
        "urls": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}